<?php

namespace App\Models;

use Illuminate\Database\Eloquent\Model;

class TodoList extends Model
{
    protected $fillable = [
        'user_id',
        'name',
        'description',
        'color',
        'is_archived',
        'done_at',
        'dead_line',
        'deleted_at',

    ];

    public function TodoItems()
    {
        return $this->hasMany(TodoItem::class);
    }
}
